server {

  root /var/www/planet;
  index index.html;

  server_name planet.oursqlcommunity.org;

  location / {
    try_files $uri $uri/ =404;
  }

  access_log /var/log/nginx/planet/access.log;
  error_log /var/log/nginx/planet/error.log;

    listen [::]:443 ssl; # managed by Certbot
    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/planet.oursqlcommunity.org-0001/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/planet.oursqlcommunity.org-0001/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot

}
server {
    if ($host = planet.oursqlcommunity.org) {
        return 301 https://$host$request_uri;
    } # managed by Certbot

  listen 80;
  listen [::]:80;

  server_name planet.oursqlcommunity.org;
    return 404; # managed by Certbot

}
